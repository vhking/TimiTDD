@model TimiTDD.Models.WorkParticipation

@{
    ViewData["Title"] = "Verifikasjon";
}

<div>
    <h3>Oversikt over fullført arbeidsøkt</h3>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Arbeidstype
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ATId.Activity)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ProjectId)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ProjectId)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ClientId)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.CId.Name)
        </dd>
        <dt>
            Arbeid utført
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Comment)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Hours)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Hours)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.WorkBreak)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.WorkBreak)
        </dd>

    </dl>
</div>
<div class="alert alert-dismissible alert-success">
    <button class="close" type="button" data-dismiss="alert">&times;</button>
    <a class="alert-link" href="#"> Se om informasjonen i arbeidsøkten stemmer med hva som ble utført.</a> Vennligst benytt "Tilbake til meny" knappen for å navigere til hovedmenyen.
</div>
<div class="btn-changes">
    <a class="btn btn-success btn-block" asp-action="Index" >Tilbake til meny</a>
</div>

